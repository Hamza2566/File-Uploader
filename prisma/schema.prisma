
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int  @id @default (autoincrement())
  username String
  email String @unique
  password String
}
model Session {
  id        String   @id @default(uuid()) // primary key
  sid       String   @unique              // session id (cookie reference)
  data      String                          // JSON-encoded session data
  expiresAt DateTime                        // when the session expires
}

